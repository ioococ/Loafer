<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="hb.rj.pojo.OrdersEntity" table="orders" schema="task">
        <id name="id">
            <column name="id"/>
<!--            <generator class="assigned"></generator>-->
            <generator class="native"/>
        </id>
        <property name="spname">
            <column name="spname" sql-type="varchar(50)" length="50" not-null="true"/>
        </property>
        <many-to-one name="customerEntity" class="hb.rj.pojo.CustomerEntity" column="customer"/>
    </class>
</hibernate-mapping>