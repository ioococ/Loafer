<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="ink.onei.bootstarter.mapper.TeachersDao">

    <resultMap type="ink.onei.bootstarter.entity.Teachers" id="TeachersMap">
        <result property="teacherId" column="teacher_id" jdbcType="INTEGER"/>
        <result property="name" column="name" jdbcType="VARCHAR"/>
        <result property="gender" column="gender" jdbcType="INTEGER"/>
        <result property="birthday" column="birthday" jdbcType="TIMESTAMP"/>
        <result property="title" column="title" jdbcType="VARCHAR"/>
        <result property="createTime" column="create_time" jdbcType="TIMESTAMP"/>
        <result property="updateTime" column="update_time" jdbcType="TIMESTAMP"/>
    </resultMap>

    <!--查询单个-->
    <select id="queryById" resultMap="TeachersMap">
        select
teacher_idnamegenderbirthdaytitlecreate_timeupdate_time
        from teachers
        where teacher_id = #{teacherId}
    </select>

    <!--查询指定行数据-->
    <select id="queryAllByLimit" resultMap="TeachersMap">
        select
teacher_idnamegenderbirthdaytitlecreate_timeupdate_time
        from teachers
        <where>
            <if test="teacherId != null">
                and teacher_id = #{teacherId}
            </if>
            <if test="name != null and name != ''">
                and name = #{name}
            </if>
            <if test="gender != null">
                and gender = #{gender}
            </if>
            <if test="birthday != null">
                and birthday = #{birthday}
            </if>
            <if test="title != null and title != ''">
                and title = #{title}
            </if>
            <if test="createTime != null">
                and create_time = #{createTime}
            </if>
            <if test="updateTime != null">
                and update_time = #{updateTime}
            </if>
        </where>
        limit #{pageable.offset}, #{pageable.pageSize}
    </select>

    <!--统计总行数-->
    <select id="count" resultType="java.lang.Long">
        select count(1)
        from teachers
        <where>
            <if test="teacherId != null">
                and teacher_id = #{teacherId}
            </if>
            <if test="name != null and name != ''">
                and name = #{name}
            </if>
            <if test="gender != null">
                and gender = #{gender}
            </if>
            <if test="birthday != null">
                and birthday = #{birthday}
            </if>
            <if test="title != null and title != ''">
                and title = #{title}
            </if>
            <if test="createTime != null">
                and create_time = #{createTime}
            </if>
            <if test="updateTime != null">
                and update_time = #{updateTime}
            </if>
        </where>
    </select>

    <!--新增所有列-->
    <insert id="insert" keyProperty="teacherId" useGeneratedKeys="true">
        insert into teachers(namegenderbirthdaytitlecreate_timeupdate_time)
        values (#{name}#{gender}#{birthday}#{title}#{createTime}#{updateTime})
    </insert>

    <insert id="insertBatch" keyProperty="teacherId" useGeneratedKeys="true">
        insert into teachers(namegenderbirthdaytitlecreate_timeupdate_time)
        values
        <foreach collection="entities" item="entity" separator=",">
        (#{entity.name}#{entity.gender}#{entity.birthday}#{entity.title}#{entity.createTime}#{entity.updateTime})
        </foreach>
    </insert>

    <insert id="insertOrUpdateBatch" keyProperty="teacherId" useGeneratedKeys="true">
        insert into teachers(namegenderbirthdaytitlecreate_timeupdate_time)
        values
        <foreach collection="entities" item="entity" separator=",">
            (#{entity.name}#{entity.gender}#{entity.birthday}#{entity.title}#{entity.createTime}#{entity.updateTime})
        </foreach>
        on duplicate key update
name = values(name)gender = values(gender)birthday = values(birthday)title = values(title)create_time = values(create_time)update_time = values(update_time)
    </insert>

    <!--通过主键修改数据-->
    <update id="update">
        update teachers
        <set>
            <if test="name != null and name != ''">
                name = #{name},
            </if>
            <if test="gender != null">
                gender = #{gender},
            </if>
            <if test="birthday != null">
                birthday = #{birthday},
            </if>
            <if test="title != null and title != ''">
                title = #{title},
            </if>
            <if test="createTime != null">
                create_time = #{createTime},
            </if>
            <if test="updateTime != null">
                update_time = #{updateTime},
            </if>
        </set>
        where teacher_id = #{teacherId}
    </update>

    <!--通过主键删除-->
    <delete id="deleteById">
        delete from teachers where teacher_id = #{teacherId}
    </delete>

</mapper>

